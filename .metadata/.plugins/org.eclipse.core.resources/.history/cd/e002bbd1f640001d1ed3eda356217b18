package Classes;

public class produtos {
	String nomeProduto;
	String quantidadeProduto;
	String valorProdutos;
	String descricaoProduto;
	
	public produtos(String nomeProduto, String quantidadeProduto, String valorProdutos, String descricaoProduto) {
		super();
		this.nomeProduto = nomeProduto;
		this.quantidadeProduto = quantidadeProduto;
		this.valorProdutos = valorProdutos;
		this.descricaoProduto = descricaoProduto;
	}

	public String getNomeProduto() {
		return nomeProduto;
	}

	public void setNomeProduto(String nomeProduto) {
		this.nomeProduto = nomeProduto;
	}

	public String getQuantidadeProduto() {
		return quantidadeProduto;
	}

	public void setQuantidadeProduto(String quantidadeProduto) {
		this.quantidadeProduto = quantidadeProduto;
	}

	public String getValorProdutos() {
		return valorProdutos;
	}

	public void setValorProdutos(String valorProdutos) {
		this.valorProdutos = valorProdutos;
	}

	public String getDescricaoProduto() {
		return descricaoProduto;
	}

	public void setDescricaoProduto(String descricaoProduto) {
		this.descricaoProduto = descricaoProduto;
	}
}
